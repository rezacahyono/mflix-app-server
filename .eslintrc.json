{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "import/no-extraneous-dependencies": ["error", {"dependencies": false}]
    }
}
